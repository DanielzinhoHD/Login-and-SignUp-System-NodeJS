Login <br>
<% if(messages.error) {%>
    <%= messages.error %>
<% } %>
<form action="/login" method="POST">
    <div>
        <label for="email">Email</label>
        <input type="email" name="email">
    </div>
    <div>
        <label for="password">Senha</label>
        <input type="password" name="password">
    </div>
    <button>Login</button>
</form>

<a href="/register">Sign Up</a>